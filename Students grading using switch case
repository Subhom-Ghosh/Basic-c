#include<stdio.h>
int main()
{
	int num;
	printf("ENTER YOUR NUMBER");
	scanf("%d",&num);
	num=num/10;
	switch (num)
	{
		case 10:printf("Your grade is O");
		break;
		case 9 : printf("Your grade is O");
		break;
		case 8 : printf("Your grade is E");
		break;
		case 7 : printf("Your grade is A");
		break;
		case 6 : printf("Your grade is B");
		break;
		case 5 : printf("Your grade is C");
		break;
		case 4 : printf("Your grade is D");
		break;
		default : printf("You are fail");
	}
	
	return 0;
}	
